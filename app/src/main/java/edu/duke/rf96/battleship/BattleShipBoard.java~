package edu.duke.rf96.battleship;

public class BattleShipBoard {
  private final int width;
  private final int height;

  public BattleShipBoard() {
    this(0, 0);
  }

  public BattleShipBoard(int w, int h) {
    this.width = w;
    this.height = h;
  }

  public int getWidth() {
    return this.width;
  }

  public int getHeight() {
    return height;
  }

}
